// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
{
  "extends": "airbnb",
  "rules": {
    "no-console": "off",
    "no-new": "off",
    "new-cap": "off",
    "strict": "off",
    "yoda": "off",
    "func-names": "off",
    "prefer-template": "off",
    "curly": "off",
    "camelcase": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "no-param-reassign": "off",
    "no-else-return": "off",
  },
  "env": {
    "mocha": true
  }
}